<%= link_to exercise do %>
  <%= content_tag :div, id: dom_id(exercise), class: "card my-3" do %>
    <h5 class="card-header"><%= exercise.name %></h5>
    <div class="card-body">
      <p><%= exercise.description %></p>
      <span class="pill"><%= exercise.category.name %></span>
    </div>
  <% end %>  
<% end %>