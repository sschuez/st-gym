<% if request.url.include?("new?") %>
  <%= turbo_stream.replace "exercise_instances" do %>
    <%= render "exercise_instances/list", exercises: @exercises %>
  <% end %>
<% else %>
  <%= turbo_stream.replace "remote_modal" do %>
    <%= render partial: "exercise_instances/form_modal" %>
  <% end %>
<% end %>
